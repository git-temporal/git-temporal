{
  "name": "@git-temporal/api-node-express",
  "private": true,
  "version": "0.5.1",
  "description": "This is an api to return git log information in JSON.",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc",
    "test": "cross-env mocha --exit -r ../../babel-register.js --recursive --opts ./test/mocha.opts ./test/unit",
    "test-debug": "cross-env mocha --inspect-brk --exit -r ../../babel-register.js --recursive --opts ./test/mocha.opts ./test/unit",
    "debug": "cross-env GTDEBUG=1 DEBUG=1 node --inspect-brk -r ../../babel-register.js src/server.ts",
    "start": "cross-env GTDEBUG=1 DEBUG=1 node -r ../../babel-register.js src/server.ts",
    "start-react": "cross-env GTDEBUG=1 DEBUG=1 GT_GIT_REPO_PATH=../../../react node -r ../../babel-register.js src/server.ts",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "api-node-express": "lib/server.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/git-temporal/git-temporal/tree/master/packages/git-log-scraper",
  "keywords": [
    "git",
    "api",
    "service",
    "json",
    "history"
  ],
  "author": "Bee Wilkerson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/git-temporal/git-temporal/issues"
  },
  "homepage": "https://github.com/git-temporal/git-temporal#readme",
  "dependencies": {
    "@git-temporal/commons": "0.5.1",
    "@git-temporal/git-diff-scraper": "0.5.1",
    "@git-temporal/git-log-scraper": "0.5.1",
    "@git-temporal/logger": "0.5.1",
    "express": "4.16.3",
    "morgan": "1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-do-expressions": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-plugin-istanbul": "5.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "bumble-util": "0.4.1",
    "chai": "4.1.2",
    "chai-string": "1.5.0",
    "cross-env": "5.2.0",
    "mocha": "5.2.0",
    "node-fetch": "2.2.0",
    "regenerator-runtime": "0.12.1",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.6.0",
    "typescript": "3.0.3",
    "underscore": "1.9.1"
  }
}
